config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Rules",
"language": "GDScript",
"path": "res://scripts/Rules.gd"
} ]
_global_script_class_icons={
"Rules": ""
}

[application]

config/name="7DRL2021"
run/main_scene="res://scripts/Dungeon.tscn"
